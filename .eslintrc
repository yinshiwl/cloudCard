{
  "extends": [
    "taro/react"
  ],
  "rules": {
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/first": "off",
    "jsx-quotes": "off",
    "react/jsx-no-undef": "error",
    "react/jsx-closing-bracket-location": "off",
    "no-unused-vars": "error"
  },
  "globals": {
    "YS_API_URL": "readonly"
  }
}